{
    "name": "@uniweb/frame-bridge",
    "version": "0.0.3",
    "description": "Promise-based iframe communication library with automatic dimension reporting, URL sync, and JSON-LD injection",
    "type": "module",
    "main": "./dist/esm/index.js",
    "module": "./dist/esm/index.js",
    "unpkg": "./dist/auto/parent.min.js",
    "jsdelivr": "./dist/auto/parent.min.js",
    "exports": {
        ".": "./dist/esm/index.js",
        "./parent": "./dist/esm/parent.js",
        "./child": "./dist/esm/child.js"
    },
    "sideEffects": false,
    "files": [
        "dist",
        "README.md",
        "LICENSE",
        "CHANGELOG.md"
    ],
    "scripts": {
        "build": "rollup -c",
        "dev": "rollup -c -w",
        "test": "vitest",
        "test:watch": "vitest --watch",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "iframe",
        "postMessage",
        "communication",
        "cross-frame",
        "messaging",
        "embed",
        "bridge",
        "url-sync",
        "deep-linking",
        "seo",
        "json-ld",
        "resize-observer",
        "parent-child",
        "cross-origin"
    ],
    "author": {
        "name": "Proximify",
        "url": "https://proximify.com"
    },
    "license": "MIT",
    "homepage": "https://github.com/uniweb/frame-bridge#readme",
    "bugs": {
        "url": "https://github.com/uniweb/frame-bridge/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/uniweb/frame-bridge.git"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-terser": "^0.4.4",
        "jsdom": "^27.3.0",
        "rollup": "^4.9.0",
        "vitest": "^1.1.0"
    }
}
